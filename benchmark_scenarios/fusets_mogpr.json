        "process_id": "mogpr"
      },
      "ndvi1": {
        "arguments": {
          "data": {
            "from_node": "mask1"
          },
          "nir": "B08",
          "red": "B04"
        },
        "process_id": "ndvi"
      },
      "toscldilationmask1": {
        "arguments": {
          "data": {
            "from_node": "loadcollection2"
          }
        },
        "process_id": "to_scl_dilation_mask"
      }
